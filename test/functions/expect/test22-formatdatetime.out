<!-- Test FormatDateTime function.
-->

Dim TheDate <br>

TheDate = "12/31/05 13:34:56" <br>

FormatDateTime(TheDate) =
	12/31/2005 1:34:56 PM<br>

FormatDateTime(TheDate, vbGeneralDate) =
